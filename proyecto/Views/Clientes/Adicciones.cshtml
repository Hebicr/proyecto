@model proyecto.Models.Adiciones
@{
    ViewBag.Title = "Adicciones";
}
<div class="container">
    <div class="row">
        <div style="border: 1px solid #d5d7dd; margin-top:30px;padding:30px; border-radius: 15px; width:400px">

            Adicciones:
            @Html.DropDownListFor(model => model.idAdiccion, ViewBag.adiccionList as SelectList, "-- Seleccione la Adiccion --", new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.idAdiccion, "", new { @class = "text-danger" })
            <a href="#" class="btn btn-primary" style="">Agregar</a>
        </div>


    </div>
</div>

