@model IEnumerable<proyecto.Models.sp_getInformacion_Cliente_Result>

@{
    ViewBag.Title = "PerfilCliente";
}

<body background="\img\Prueba.png" style="width:100%; height:100%; background-size: cover;">
    <div class="container justify-content-center" style="width:500px; background-color:white; border-radius:10px 10px ">
        <h2>Datos del cliente</h2>
        <div>
            <hr />
            <dl class="dl-horizontal">
                @foreach (var item in Model)
                {
                    <dt>
                        Cedula:
                    </dt>

                    <dd>
                        @Html.DisplayFor(modelItem => item.Cedula)
                    </dd>

                    <dt>
                        Nombre Completo:
                    </dt>

                    <dd>
                        @Html.DisplayFor(modelItem => item.Nombre) @Html.DisplayFor(modelItem => item.PrimerApellido) @Html.DisplayFor(modelItem => item.SegundoApellido)
                    </dd>

                    <dt>
                        Fecha Nacimiento:
                    </dt>

                    <dd>
                        @Html.DisplayFor(modelItem => item.FechadeNacimiento)
                    </dd>

                    <dt>
                        Dirección Fisica:
                    </dt>

                    <dd>
                        @Html.DisplayFor(modelItem => item.DireccionFisica)
                    </dd>

                    <dt>
                        Telefono:
                    </dt>

                    <dd>
                        @Html.DisplayFor(modelItem => item.TelefonoPrincipal)
                    </dd>

                    <dt>
                        Email:
                    </dt>

                    <dd>
                        @Html.DisplayFor(modelItem => item.CorreoElectronico)
                    </dd>

                   
                }
                </dl>
        </div>
        <p class="col-md-6" style="">
            <a href='@Url.Action("Index", "Home")'>
                <input type='button' value='Regresar' class="btn btn-danger" />
            </a>
        </p>

    </div>
</body>
